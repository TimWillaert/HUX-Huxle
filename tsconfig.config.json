{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "playwright.config.*",
    "src/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "node"
    ],
    "noEmit": true,
    "strict": false,
  },
  "exclude": [
    "src/**/*.vue"
  ]
}